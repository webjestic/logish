{
    "env": {
        "es2021": true,
        "node": true
        //"jest/globals": true
    },
    //"extends": ["eslint:recommended", "plugin:jest/recommended"],
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "strict": 1,
        "no-duplicate-imports": 1,
        "no-unused-private-class-members": 1
    }
}
